{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:build:server dev:server dev:build:client",
    "dev:build:server": "NODE_ENV='development' RELATION='server' webpack --watch",
    "dev:server": "nodemon --inspect --watch build --exec 'node build/bundle.js'",
    "dev:build:client": "NODE_ENV='development' RELATION='client' webpack  --watch",
    "prod:build": "npm-run-all --parallel prod:build:server prod:build:client",
    "prod:build:server": "NODE_ENV='production' RELATION='server' webpack ",
    "prod:build:client": "NODE_ENV='production' RELATION='client' webpack "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "1.7.3",
    "concurrently": "3.6.1",
    "express": "4.16.3",
    "express-http-proxy": "1.2.0",
    "husky": "^0.14.3",
    "lodash": "4.17.10",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "1.5.0",
    "webpack": "4.17.1",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "webpack-cli": "^3.1.0"
  }
}
